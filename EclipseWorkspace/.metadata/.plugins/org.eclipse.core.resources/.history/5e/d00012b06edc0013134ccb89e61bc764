package worms.model;

public class ExpressionsSquareRoot extends Expression {
	
	public ExpressionsSquareRoot(int line, int column, Expression e) {
		super(line,column,e);
	}

	@Override
	public Type<Double> evaluate() {
		
		if ( !(e.evaluate().getValue() instanceof Number) )
			getStatement().getRootProgram().typeErrorOccurred();
		return new Type<Double>(Math.sqrt((double)((Type<Double>)e.evaluate()).getValue()));
	}

}
