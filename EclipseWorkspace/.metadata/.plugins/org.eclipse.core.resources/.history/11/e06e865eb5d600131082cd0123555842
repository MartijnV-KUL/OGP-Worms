package worms.model;

public class ExpressionType extends Expression {
	
	private final Type<?> t;
	
	public ExpressionType(int line, int column, Type<?> t) {
		//TODO the assigment says that an expression can be a double, but it doesn't say it can be an entity
		super(line,column,null);
		this.t = t;
	}
	
	public Type<?> evaluate() {
		return t;
	}

}
