package worms.model;

public class ExpressionDouble extends Expression {
	
	private final Type<?> t;
	
	public ExpressionDouble(int line, int column, Type<?> t) {
		//TODO the assigment says that an expression can be a double, but it doesn't say it can be an entity. How do you then call GETX on an expression if it can't be a worm? through the variables?
		super(line,column,null);
		this.t = t;
	}
	
	public Type<?> evaluate() {
		return t;
	}

}
