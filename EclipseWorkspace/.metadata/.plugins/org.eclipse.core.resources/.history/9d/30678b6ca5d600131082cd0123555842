package worms.model;

public class StatementAssignment extends Statement {
	
	private String variable;
	
	public StatementAssignment(String variable, Expression e) {
		super(new Statement[0], new Expression[] {e});
		this.variable = variable;
	}

	@Override
	public void execute() {
		getProgram().assignVariable(variable, getExpressions().get(0));
	}

	@Override
	public boolean containsAstionStatement() {
		// TODO Auto-generated method stub
		return false;
	}

}
