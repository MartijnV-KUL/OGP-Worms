package worms.model;

public class Bazooka extends Weapon {
	
	public Bazooka() {
		projectileMass   = 0.300;
		hitPointsDamage  = 80;
		actionPointsCost = 50;
	}

	@Override
	public double getForce(int propulsionYield) {
		// Linear scaling
		double p = ((double)propulsionYield)/100;
		return 2.5 + 7*p;
	}

}
